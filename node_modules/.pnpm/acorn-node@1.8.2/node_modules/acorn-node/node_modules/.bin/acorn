#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/opt/lampp/htdocs/E-Learning/node_modules/.pnpm/acorn@7.4.1/node_modules/acorn/bin/node_modules:/opt/lampp/htdocs/E-Learning/node_modules/.pnpm/acorn@7.4.1/node_modules/acorn/node_modules:/opt/lampp/htdocs/E-Learning/node_modules/.pnpm/acorn@7.4.1/node_modules:/opt/lampp/htdocs/E-Learning/node_modules/.pnpm/node_modules:/opt/lampp/htdocs/E-Learning/node_modules:/opt/lampp/htdocs/node_modules:/opt/lampp/node_modules:/opt/node_modules:/node_modules:/opt/lampp/htdocs/E-Learning/node_modules/.pnpm/acorn-node@1.8.2/node_modules/acorn-node/node_modules:/opt/lampp/htdocs/E-Learning/node_modules/.pnpm/acorn-node@1.8.2/node_modules"
else
  export NODE_PATH="$NODE_PATH:/opt/lampp/htdocs/E-Learning/node_modules/.pnpm/acorn@7.4.1/node_modules/acorn/bin/node_modules:/opt/lampp/htdocs/E-Learning/node_modules/.pnpm/acorn@7.4.1/node_modules/acorn/node_modules:/opt/lampp/htdocs/E-Learning/node_modules/.pnpm/acorn@7.4.1/node_modules:/opt/lampp/htdocs/E-Learning/node_modules/.pnpm/node_modules:/opt/lampp/htdocs/E-Learning/node_modules:/opt/lampp/htdocs/node_modules:/opt/lampp/node_modules:/opt/node_modules:/node_modules:/opt/lampp/htdocs/E-Learning/node_modules/.pnpm/acorn-node@1.8.2/node_modules/acorn-node/node_modules:/opt/lampp/htdocs/E-Learning/node_modules/.pnpm/acorn-node@1.8.2/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../acorn@7.4.1/node_modules/acorn/bin/acorn" "$@"
else
  exec node  "$basedir/../../../../../acorn@7.4.1/node_modules/acorn/bin/acorn" "$@"
fi
