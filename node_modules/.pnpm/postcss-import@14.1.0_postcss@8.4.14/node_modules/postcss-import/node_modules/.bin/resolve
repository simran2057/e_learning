#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/opt/lampp/htdocs/E-Learning/node_modules/.pnpm/resolve@1.22.0/node_modules/resolve/bin/node_modules:/opt/lampp/htdocs/E-Learning/node_modules/.pnpm/resolve@1.22.0/node_modules/resolve/node_modules:/opt/lampp/htdocs/E-Learning/node_modules/.pnpm/resolve@1.22.0/node_modules:/opt/lampp/htdocs/E-Learning/node_modules/.pnpm/node_modules:/opt/lampp/htdocs/E-Learning/node_modules:/opt/lampp/htdocs/node_modules:/opt/lampp/node_modules:/opt/node_modules:/node_modules:/opt/lampp/htdocs/E-Learning/node_modules/.pnpm/postcss-import@14.1.0_postcss@8.4.14/node_modules/postcss-import/node_modules:/opt/lampp/htdocs/E-Learning/node_modules/.pnpm/postcss-import@14.1.0_postcss@8.4.14/node_modules"
else
  export NODE_PATH="$NODE_PATH:/opt/lampp/htdocs/E-Learning/node_modules/.pnpm/resolve@1.22.0/node_modules/resolve/bin/node_modules:/opt/lampp/htdocs/E-Learning/node_modules/.pnpm/resolve@1.22.0/node_modules/resolve/node_modules:/opt/lampp/htdocs/E-Learning/node_modules/.pnpm/resolve@1.22.0/node_modules:/opt/lampp/htdocs/E-Learning/node_modules/.pnpm/node_modules:/opt/lampp/htdocs/E-Learning/node_modules:/opt/lampp/htdocs/node_modules:/opt/lampp/node_modules:/opt/node_modules:/node_modules:/opt/lampp/htdocs/E-Learning/node_modules/.pnpm/postcss-import@14.1.0_postcss@8.4.14/node_modules/postcss-import/node_modules:/opt/lampp/htdocs/E-Learning/node_modules/.pnpm/postcss-import@14.1.0_postcss@8.4.14/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../resolve@1.22.0/node_modules/resolve/bin/resolve" "$@"
else
  exec node  "$basedir/../../../../../resolve@1.22.0/node_modules/resolve/bin/resolve" "$@"
fi
