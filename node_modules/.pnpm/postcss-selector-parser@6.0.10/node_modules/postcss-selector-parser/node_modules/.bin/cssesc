#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/opt/lampp/htdocs/E-Learning/node_modules/.pnpm/cssesc@3.0.0/node_modules/cssesc/bin/node_modules:/opt/lampp/htdocs/E-Learning/node_modules/.pnpm/cssesc@3.0.0/node_modules/cssesc/node_modules:/opt/lampp/htdocs/E-Learning/node_modules/.pnpm/cssesc@3.0.0/node_modules:/opt/lampp/htdocs/E-Learning/node_modules/.pnpm/node_modules:/opt/lampp/htdocs/E-Learning/node_modules:/opt/lampp/htdocs/node_modules:/opt/lampp/node_modules:/opt/node_modules:/node_modules:/opt/lampp/htdocs/E-Learning/node_modules/.pnpm/postcss-selector-parser@6.0.10/node_modules/postcss-selector-parser/node_modules:/opt/lampp/htdocs/E-Learning/node_modules/.pnpm/postcss-selector-parser@6.0.10/node_modules"
else
  export NODE_PATH="$NODE_PATH:/opt/lampp/htdocs/E-Learning/node_modules/.pnpm/cssesc@3.0.0/node_modules/cssesc/bin/node_modules:/opt/lampp/htdocs/E-Learning/node_modules/.pnpm/cssesc@3.0.0/node_modules/cssesc/node_modules:/opt/lampp/htdocs/E-Learning/node_modules/.pnpm/cssesc@3.0.0/node_modules:/opt/lampp/htdocs/E-Learning/node_modules/.pnpm/node_modules:/opt/lampp/htdocs/E-Learning/node_modules:/opt/lampp/htdocs/node_modules:/opt/lampp/node_modules:/opt/node_modules:/node_modules:/opt/lampp/htdocs/E-Learning/node_modules/.pnpm/postcss-selector-parser@6.0.10/node_modules/postcss-selector-parser/node_modules:/opt/lampp/htdocs/E-Learning/node_modules/.pnpm/postcss-selector-parser@6.0.10/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../cssesc@3.0.0/node_modules/cssesc/bin/cssesc" "$@"
else
  exec node  "$basedir/../../../../../cssesc@3.0.0/node_modules/cssesc/bin/cssesc" "$@"
fi
