#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/opt/lampp/htdocs/E-Learning/node_modules/.pnpm/mini-svg-data-uri@1.4.4/node_modules/mini-svg-data-uri/node_modules:/opt/lampp/htdocs/E-Learning/node_modules/.pnpm/mini-svg-data-uri@1.4.4/node_modules:/opt/lampp/htdocs/E-Learning/node_modules/.pnpm/node_modules:/opt/lampp/htdocs/E-Learning/node_modules:/opt/lampp/htdocs/node_modules:/opt/lampp/node_modules:/opt/node_modules:/node_modules:/opt/lampp/htdocs/E-Learning/node_modules/.pnpm/flowbite@1.4.7/node_modules/flowbite/node_modules:/opt/lampp/htdocs/E-Learning/node_modules/.pnpm/flowbite@1.4.7/node_modules"
else
  export NODE_PATH="$NODE_PATH:/opt/lampp/htdocs/E-Learning/node_modules/.pnpm/mini-svg-data-uri@1.4.4/node_modules/mini-svg-data-uri/node_modules:/opt/lampp/htdocs/E-Learning/node_modules/.pnpm/mini-svg-data-uri@1.4.4/node_modules:/opt/lampp/htdocs/E-Learning/node_modules/.pnpm/node_modules:/opt/lampp/htdocs/E-Learning/node_modules:/opt/lampp/htdocs/node_modules:/opt/lampp/node_modules:/opt/node_modules:/node_modules:/opt/lampp/htdocs/E-Learning/node_modules/.pnpm/flowbite@1.4.7/node_modules/flowbite/node_modules:/opt/lampp/htdocs/E-Learning/node_modules/.pnpm/flowbite@1.4.7/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../mini-svg-data-uri@1.4.4/node_modules/mini-svg-data-uri/cli.js" "$@"
else
  exec node  "$basedir/../../../../../mini-svg-data-uri@1.4.4/node_modules/mini-svg-data-uri/cli.js" "$@"
fi
