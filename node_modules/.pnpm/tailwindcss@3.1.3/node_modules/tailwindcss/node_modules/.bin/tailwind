#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/opt/lampp/htdocs/E-Learning/node_modules/.pnpm/tailwindcss@3.1.3/node_modules/tailwindcss/lib/node_modules:/opt/lampp/htdocs/E-Learning/node_modules/.pnpm/tailwindcss@3.1.3/node_modules/tailwindcss/node_modules:/opt/lampp/htdocs/E-Learning/node_modules/.pnpm/tailwindcss@3.1.3/node_modules:/opt/lampp/htdocs/E-Learning/node_modules/.pnpm/node_modules:/opt/lampp/htdocs/E-Learning/node_modules:/opt/lampp/htdocs/node_modules:/opt/lampp/node_modules:/opt/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/opt/lampp/htdocs/E-Learning/node_modules/.pnpm/tailwindcss@3.1.3/node_modules/tailwindcss/lib/node_modules:/opt/lampp/htdocs/E-Learning/node_modules/.pnpm/tailwindcss@3.1.3/node_modules/tailwindcss/node_modules:/opt/lampp/htdocs/E-Learning/node_modules/.pnpm/tailwindcss@3.1.3/node_modules:/opt/lampp/htdocs/E-Learning/node_modules/.pnpm/node_modules:/opt/lampp/htdocs/E-Learning/node_modules:/opt/lampp/htdocs/node_modules:/opt/lampp/node_modules:/opt/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../lib/cli.js" "$@"
else
  exec node  "$basedir/../../lib/cli.js" "$@"
fi
